<% content_for :title, t('family_products.title') %>
<% content_for :subtitle, t('crud.edit.title_id', id: @product_family.id) %>

<% if notice %>
    <div class="alert alert-info alert-dismissible" role="alert" id="notice">
      <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
      <%= notice %>
    </div>
<% end %>

<%= render 'form_edit', product_family: @product_family %>
